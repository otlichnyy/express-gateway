{
  "name": "express-ts-boilerplate",
  "version": "1.0.0",
  "description": "minimal boilerplate",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "dev:debug": "nodemon --config ./config/nodemon-debug.json",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/src/index.js",
    "build": "rm -rf dist && tsc -p tsconfig.prod.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "check-types": "bash -c tsc",
    "eslint": "eslint .",
    "lint:fix": "npm run eslint --fix .",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production'){process.exit(1)} \" || husky install",
    "resolve": "npx npm-force-resolutions",
    "lint-staged": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-otlichnyy:otlichnyy/express-ts-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@graphql-tools/delegate": "^8.8.0",
    "@graphql-tools/schema": "^8.5.0",
    "@graphql-tools/wrap": "^8.5.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.3",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.0.0",
    "express-graphql": "^0.12.0",
    "graphql": "^15.8.0",
    "graphql-tools": "^8.3.0",
    "husky": "^8.0.0",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.2",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "ts-to-zod": "^1.11.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.1",
    "express-rate-limit": "^6.4.0",
    "helmet": "^5.1.0",
    "http-status": "^1.5.2",
    "morgan": "^1.10.0",
    "node-fetch": "^3.2.6",
    "rate-limit-redis": "^3.0.1",
    "redis": "^4.1.0",
    "supertokens-node": "^11.0.0",
    "winston": "^3.8.1",
    "xss-clean": "^0.1.1",
    "zod": "^3.17.3"
  },
  "lint-staged": {
    "*.(ts|js)": [
      "npm run lint:fix",
      "npm run check-types"
    ],
    "*.{js,md,ts}": "prettier --write --ignore-unknown"
  }
}
